  "scripts": {
    "build:coffee": "coffee --compile --transpile --inline-map ./src/server",
    "build:coffee:watch": "coffee --compile --transpile --inline-map --watch ./src/server",
    "build:tsc": "tsc -p ./src/client",
    "build": "npm run build:coffee && npm run build:tsc",
    "start": "parallel build:coffee:watch start:dev",
    "start:dev": "nodemon ./src/server/server.js",
    "start:prod": "pm2 start pm2.config.yaml",
    "start:debug": "nodemon --inspect ./src/server/server.js",
    "start:db": "nodemon ./src/server/db-server.js",
    "serve": "node ./src/server/server.js",
    "serve:prod": "NODE_ENV=production node ./src/server/server.js",
    "serve:db": "node ./src/server/db-server.js",
    "lint": "tslint ./src/client/**/*.ts -t verbose",
    "find:ts": "find ./src/client -name '*.ts' -type f",
    "find:js": "find ./src/client \\( -name '*.js' -or -name '*.map' \\) -type f",
    "delete:js": "find ./src/client -name \"*.js\" -type f -delete",
    "delete:map": "find ./src/client -name '*.map' -type f -delete",
    "delete": "npm run delete:js && npm run delete:map",
    "delete:node": "rm -rf ./node_modules && rm yarn.lock",
    "flush:node": "rm -rf ./node_modules && rm yarn.lock && npm install",
    "---end---": "---end---"
  }
